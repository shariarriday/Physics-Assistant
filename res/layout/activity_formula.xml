<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgmain"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/VectorTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="Vector"
        android:layout_gravity="center"
        android:textStyle="bold"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#000000" />

    <Button
        android:id="@+id/vector101"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@drawable/vector101" />

    <Button
        android:id="@+id/vector102"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@drawable/vector102" />

    <Button
        android:id="@+id/vector103"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/vector103" />


    <Button
        android:id="@+id/vector104"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="@drawable/vector104" />

    <Button
        android:id="@+id/vector105"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="@drawable/vector105" />

    <Button
        android:id="@+id/vector106"
       android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/vector106" />

    <Button
        android:id="@+id/vector107"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/vector107" />

    <Button
        android:id="@+id/vector108"
     android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/vector108" />
   <Button
        android:id="@+id/vector109"
       android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/vector109"
        />
    <Button
        android:id="@+id/vector110"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@drawable/vector110" />

</LinearLayout>